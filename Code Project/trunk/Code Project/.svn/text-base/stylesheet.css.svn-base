@charset "utf-8";
h1 {
	color: #00CC99;
	font-family: Cambria, "Hoefler Text", "Liberation Serif", Times, "Times New Roman", serif;
	font-style: oblique;
	font-variant: normal;
	text-align: center;
	
}
body {
	text-color: #3948393;
	font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
	color: #006633;
	font-style: oblique;
	font-variant: normal;
	font-weight: lighter;
	font-size: medium;
	text-align: left;
	text-decoration: none;
	text-shadow: 0px 0px #003333;
	background-color: #FFCC33;
}
h2 {
	color: #3300CC;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-style: oblique;
	font-variant: normal;
	font-weight: lighter;
	font-size: x-large;
	text-align: center;
}
